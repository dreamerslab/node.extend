{
	"root": true,

	"extends": "@ljharb",

	"rules": {
		"complexity": 0,
		"eqeqeq": [2, "allow-null"],
		"indent": [2, 2],
		"max-depth": [2, 5],
		"multiline-comment-style": 0,
		"no-continue": [1],
		"no-restricted-syntax": [2, "BreakStatement", "DebuggerStatement", "LabeledStatement", "WithStatement"],
		"sort-keys": 0,
	},
}
